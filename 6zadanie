#include <iostream>

using namespace std;

int main()
{
  int n,i,j,sum;
  const int m = 3;
  int arr[m][m] = {};
  for (i = 0; i < m; i++){
    for (j = 0; j < m; j++){
      cin » n;
      if(n != 0 && n != 1 && n != 5 && n != 11){
        cout « "error";
        return 0;
      }
      else
        arr[i][j] = n;
    }
  }

  i = 0;
  j = 0;
  sum = 0;

  for (i = 0; i < m; i++){
    for (j = 0; j < m; j++){
      if (arr[i][j] != arr[i+1][j] && arr[i][j+1] != arr[i+1][j+1] && arr[i][j] != arr[i][j+1] && arr[i][j] != arr[i+1][j+1] && arr[i][j+1] != arr[i+1][j]){
        sum++;
      }
    }
  }
  
  cout « sum « endl;
}
